################# Long Frames #################
long_frames.enabled=true


## short_code        :: [WORKING] -- represents the root command for long frames (i.e. 666 & 777)
##
## sub_short_codes   :: [OPTIONAL]
##                      [SYNTAX]  -- comma seperated integer value/s
##                      [WORKING] -- represents sub short codes to be associated with long frames after root command (i.e. *777*34 is for TRANSFER
##                                   whereas 777 is short_code[root command] and 34 is sub_short_code)
##
## append_arguments  :: [OPTIONAL]
##                      [SYNTAX]  -- comma separated integer value/s
##                      [WORKING] -- appends configured values at the end of input parameters provided/input by end user
##
## commands_regexp   :: [MANDATORY]
##                      [WORKING] -- regular expression for validating the end user input while considering the append_arguments
##
## full_param_length :: [MANDATORY]
##                      [WORKING] -- number of parameters excluding root command/short_code while considering the append_arguments
##
## remove_indices    :: [OPTIONAL]
##                      [SYNTAX]  -- comma separated integer value/s
##                      [WORKING] -- to remove hard coded element, by end user, at specific index excluding short_code for index evaluation

# GENERATE ANONYMOUS ID
# INPUT ====> *666#
# COMMAND ====> http://localhost:8680/cc/CC?src=923452255990&sessionid=noman&cmd=666
long_frames.1.short_code=666
long_frames.1.commands_regexp=\\*666\\*1#
long_frames.1.full_param_length=1
long_frames.1.append_arguments=1

# TRANSFER
# *777*34*923450600000*50*1234#
# INPUT ====> *777*34*RetlD*PIN*MSISDN*RetlD*AMOUNT#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=34&p2=1&p3=1234&p4=923450600000&p5=1&p6=50
long_frames.2.short_code=777
long_frames.2.sub_short_codes=34
long_frames.2.commands_regexp=\\*777\\*34\\*(1)\\*\\d{4}(.*?)\\*(0092|92|0)(3)[0-9]{9}\\*(1)\\*[0-9]+#
long_frames.2.full_param_length=6
long_frames.2.remove_indices=2,5

# TOP-UP
# *777*30*1*1234*923450600000*50#
# INPUT ====> *777*30*RetlD*PIN*MSISDN*AMOUNT#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=30&p2=1&p3=1234&p4=923451234565&p5=50
long_frames.3.short_code=777
long_frames.3.sub_short_codes=30
long_frames.3.commands_regexp=\\*777\\*30\\*(1)\\*\\d{4}(.*?)\\*((0092|92|0)(3)[0-9]{9}|[0-9]{11})\\*[0-9]+#
long_frames.3.full_param_length=5
long_frames.3.remove_indices=2

# REVERSAL
# *777*55*1234*923450600000#
# INPUT ====> *777*55*PIN*Transaction Reference Number#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=55&p2=1234&p3=2022031412135720101000385
long_frames.4.short_code=777
long_frames.4.sub_short_codes=55
long_frames.4.commands_regexp=\\*777\\*55\\*\\d{4}(.*?)\\*[0-9]{25,30}#
long_frames.4.full_param_length=3

# Change Pin
# *777*33*1*1234*4321*4321#
# INPUT ====> *777*33*RetlD*PIN*NewPIN1*NewPIN2#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=33&p2=1&p3=1234&p4=4321&p5=4321
long_frames.5.short_code=777
long_frames.5.sub_short_codes=33
long_frames.5.commands_regexp=\\*777\\*33\\*(1)\\*\\d{4}(.*?)\\*\\d{4}(.*?)\\*\\d{4}(.*?)#
long_frames.5.full_param_length=5
long_frames.5.remove_indices=2

# Reset PIN to downstream
# *777*37*1*1234*923450600000#
# INPUT ====> *777*37*RetlD*PIN*MSISDN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=37&p2=1&p3=1234&p4=923450600000
long_frames.6.short_code=777
long_frames.6.sub_short_codes=37
long_frames.6.commands_regexp=\\*777\\*37\\*(1)\\*\\d{4}(.*?)\\*(46|44|358|92|48)?[0-9]{9,12}#
long_frames.6.full_param_length=4
long_frames.6.remove_indices=2

# Modify Language
# *777*40*ar*1234#
# INPUT ====> *777*40*Language*PIN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=40&p2=ar&p3=1234
long_frames.7.short_code=777
long_frames.7.sub_short_codes=40
long_frames.7.commands_regexp=\\*777\\*40\\*[a-zA-Z]{2}\\*\\d{4}(.*?)#
long_frames.7.full_param_length=3

# e-load balance inquiry
# *777*31*1*1234#
# INPUT ====> *777*31*RetlD*PIN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=31&p2=1&p3=1234
long_frames.8.short_code=777
long_frames.8.sub_short_codes=31
long_frames.8.commands_regexp=\\*777\\*31\\*(1)\\*\\d{4}(.*?)#
long_frames.8.full_param_length=3
long_frames.8.remove_indices=2

# transaction inquiry by ID
# *777*41*2022021421143858301000003*1234#
# INPUT ====> *777*41*Transaction Reference Number*PIN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=41&p2=2022021421143858301000003&p3=1234
long_frames.9.short_code=777
long_frames.9.sub_short_codes=41
long_frames.9.commands_regexp=\\*777\\*41\\*[0-9]{25,30}\\*\\d{4}(.*?)#
long_frames.9.full_param_length=3

# last topup transaction inquiry
# *777*35*1*1234#
# INPUT ====> *777*35*RetlD*PIN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=35&p2=1&p3=1234
long_frames.10.short_code=777
long_frames.10.sub_short_codes=35
long_frames.10.commands_regexp=\\*777\\*35\\*(1)\\*\\d{4}(.*?)#
long_frames.10.full_param_length=3
long_frames.10.remove_indices=2

# last transfer transaction inquiry
# *777*36*1*1234#
# INPUT ====> *777*36*RetlD*PIN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=36&p2=1&p3=1234
long_frames.11.short_code=777
long_frames.11.sub_short_codes=36
long_frames.11.commands_regexp=\\*777\\*36\\*(1)\\*\\d{4}(.*?)#
long_frames.11.full_param_length=3
long_frames.11.remove_indices=2

# block reseller
# *777*38*1234*923450600000#
# INPUT ====> *777*38*PIN*MSISDN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=38&p2=1234&p3=923450600000
long_frames.12.short_code=777
long_frames.12.sub_short_codes=38
long_frames.12.commands_regexp=\\*777\\*38\\*\\d{4}(.*?)\\*(46|44|358|92|48)?[0-9]{9,12}#
long_frames.12.full_param_length=3

# unblock reseller
# *777*39*1234*923450600000#
# INPUT ====> *777*39*PIN*MSISDN#
# COMMAND ====> http://localhost:8680/cc/CC?src=923450100000&sessionid=noman&cmd=777&p1=39&p2=1234&p3=923450600000
long_frames.13.short_code=777
long_frames.13.sub_short_codes=39
long_frames.13.commands_regexp=\\*777\\*39\\*\\d{4}(.*?)\\*(46|44|358|92|48)?[0-9]{9,12}#
long_frames.13.full_param_length=3


